{
  "name": "@yab/sequencer",
  "version": "0.1.8",
  "license": "MIT",
  "private": false,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "lint": "eslint ./src",
    "test": "node --experimental-vm-modules --experimental-wasm-modules --experimental-wasm-threads ../../node_modules/jest/bin/jest.js ./src",
    "test:ci": "node --experimental-vm-modules --experimental-wasm-modules --experimental-wasm-threads ../../node_modules/jest/bin/jest.js ./src",
    "test:watch": "node --experimental-vm-modules --experimental-wasm-modules --experimental-wasm-threads ../../node_modules/jest/bin/jest.js ./src --watch",
    "start": "npm run build && node --experimental-vm-modules --experimental-wasm-modules --experimental-wasm-threads ./dist/index.js"
  },
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "snarkyjs": "0.10.0",
    "tsyringe": "^4.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17"
  },
  "dependencies": {
    "class-validator": "^0.14.0",
    "express": "^4.18.2",
    "graphql": "15.3.0",
    "graphql-yoga": "^3.9.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  }
}
